<%- include("header") -%> 

    <div class="box" id="heading"><h1><%= title %> <%= KindDay %></h1></div>
    <div class="box">
        
        <% for( let i = 0; i < todo.length; i++ ) { %>
            <div class="item"> 
                <input type="checkbox" >
             <p> <%= todo[i] %> </p>
            </div>
        <% } %>
       
        
        <form action="/" method="post" class="item">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit" value=<%= title %> name="btn">+</button>
        </form>
  
    </div>

<%- include("footer") -%> 